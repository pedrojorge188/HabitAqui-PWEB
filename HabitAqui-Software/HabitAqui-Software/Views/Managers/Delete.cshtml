@model HabitAqui_Software.Models.Manager

@{
    ViewData["Title"] = "Delete";
}

<div class="container-fluid">
    <div class="row">
        <div class="col">
            <h1 class="display-4 text-left text-md-left">@ViewData["Title"]</h1>
            <h3 class="display-4 text-left text-md-left">Are you sure you want to delete this?</h3>
            <hr />
        </div>
    </div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.locador)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.locador.Id)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
